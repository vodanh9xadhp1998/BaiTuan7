package ClientServer;

import java.util.concurrent.Callable;

public class ThreadCallable implements Callable<Integer> {
    
    private int a;
    private int b;
 
    public ThreadCallable(int a, int b) {
        this.a = a;
        this.b = b;
    }
    
    public Integer call() throws Exception {
        return a + b;
    }
}